{
  "name": "ton-swap-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "bignumber.js": "^9.0.1",
    "i18next": "^21.5.3",
    "node-sass": "^4.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "6",
    "react-scripts": "^5.0.0",
    "recharts": "^2.1.6",
    "redux-thunk": "^2.4.0",
    "simple-uniswap-sdk": "^3.6.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "docker:build": "docker build -f Dockerfile.prod -t  ton-swap:prod .",
    "docker:build:dev": "docker build -t ton-swap:dev .",
    "docker:run": "docker run -it --rm -p 1337:80 ton-swap:prod",
    "docker:run:dev": "docker run -it --rm -v ${PWD}:/app -v /app/node_modules -p 3001:3000 -e CHOKIDAR_USEPOLLING=true ton-swap:dev",
    "i18n": "i18next-scanner",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "lint",
    "prettier": "prettier \"src/**/*\" --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "i18next-scanner": "^3.1.0",
    "i18next-scanner-typescript": "^1.0.6",
    "prettier": "^2.5.1",
    "source-map-explorer": "^2.5.2"
  }
}
